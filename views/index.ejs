            <script>
                let currentCard = 0;
                const totalCards = 3;
                const prevBtn = document.getElementById('prevBtn');
                const nextBtn = document.getElementById('nextBtn');

                function updateButtonVisibility() {
                    prevBtn.style.display = currentCard === 0 ? 'none' : 'block';
                    nextBtn.style.display = currentCard === totalCards - 1 ? 'none' : 'block';
                }

                function slideCards(direction) {
                    const cardsContainer = document.getElementById('mobileCanteenCards');
                    const cardWidth = cardsContainer.offsetWidth;
                    
                    if (direction === 'right' && currentCard < totalCards - 1) {
                        currentCard++;
                    } else if (direction === 'left' && currentCard > 0) {
                        currentCard--;
                    }
                    
                    cardsContainer.style.transform = `translateX(-${currentCard * cardWidth}px)`;
                    updateButtonVisibility();
                }

                // Initialize button visibility
                updateButtonVisibility();
