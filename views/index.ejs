    <script>
        document.addEventListener('DOMContentLoaded', function () {
            // Remove custom scrollbar styling
            document.body.style.overflow = 'auto';
            document.getElementById('viewport').style.overflow = 'visible';
            document.getElementById('js-scroll-content').style.overflow = 'visible';

            const teamContainer = document.querySelector('.team-members-container');
            const teamMembers = Array.from(teamContainer.children);
            const viewMoreBtn = document.getElementById('viewMoreBtn');
            const spacer = document.getElementById('spacer');
            let isExpanded = false;

            // Remove Dr. Sripath Roy Koganti from the array
            const drSripathElement = teamMembers.find(member =>
                member.querySelector('.h3-title').textContent.trim() === 'Dr Sripath Roy Koganti'
            );
            const drSripathIndex = teamMembers.indexOf(drSripathElement);
            if (drSripathIndex > -1) {
                teamMembers.splice(drSripathIndex, 1);
            }

            // Shuffle the remaining team members
            for (let i = teamMembers.length - 1; i > 0; i--) {
                const j = Math.floor(Math.random() * (i + 1));
                [teamMembers[i], teamMembers[j]] = [teamMembers[j], teamMembers[i]];
            }

            // Add Dr. Sripath Roy Koganti back to the beginning of the array
            if (drSripathElement) {
                teamMembers.unshift(drSripathElement);
            }

            // Reappend the shuffled members
            teamMembers.forEach(member => teamContainer.appendChild(member));

            function updateVisibility() {
                teamMembers.forEach((member, index) => {
                    if (index < 6) {
                        member.classList.remove('hidden-team-member');
                        member.style.display = '';
                    } else {
                        member.classList.add('hidden-team-member');
                        member.style.display = isExpanded ? '' : 'none';
                    }
                });

                // Calculate total height of hidden members
                const hiddenMembers = teamMembers.slice(6);
                const totalHeight = hiddenMembers.reduce((height, member) => {
                    return height + member.offsetHeight;
                }, 0);

                // Add transition to spacer
                spacer.style.transition = 'height 0.3s ease-in-out';

                // Update spacer height based on expansion state
                if (isExpanded) {
                    // Add extra padding for both mobile and desktop
                    spacer.style.height = `${totalHeight + 50}px`;
                } else {
                    spacer.style.height = '0';
                }
            }

            // Initial visibility setup
            updateVisibility();

            viewMoreBtn.addEventListener('click', function () {
                isExpanded = !isExpanded;
                updateVisibility();
                viewMoreBtn.textContent = isExpanded ? 'View Less' : 'View More';

                if (isExpanded) {
                    // Scroll to the 7th team member (index 6)
                    const seventhMember = teamMembers[6];
                    seventhMember.scrollIntoView({ behavior: 'smooth', block: 'start' });
                } else {
                    // Scroll to the "View More" button
                    viewMoreBtn.scrollIntoView({ behavior: 'smooth', block: 'start' });
                }
            });

            // Update spacer height on window resize
            window.addEventListener('resize', function() {
                if (isExpanded) {
                    updateVisibility();
                }
            });
        });
    </script>

