<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Payment Failed - KL Eats</title>
    <link rel="icon" href="/images/file.png" type="image/x-icon">

    <!-- for icons  -->
    <link rel="stylesheet" href="https://unicons.iconscout.com/release/v4.0.0/css/line.css">
    <!-- bootstrap  -->
    <link rel="stylesheet" href="/stylesheets/bootstrap.min.css">
    <!-- for swiper slider  -->
    <link rel="stylesheet" href="/stylesheets/swiper-bundle.min.css">

    <!-- fancy box  -->
    <link rel="stylesheet" href="/stylesheets/jquery.fancybox.min.css">
    <!-- custom css  -->
    <link rel="stylesheet" href="/stylesheets/style.css">
    <style>
        .content-padding {
            padding-top: 120px; /* Adjust this value as needed */
        }
        .failed-image {
            max-width: 300px;
            margin: 20px auto;
        }
    </style>
</head>

<body class="body-fixed">
    <!-- start of header  -->
    <header class="site-header">
        <div class="container">
            <div class="row">
                <div class="col-lg-2">

                </div>
                <div class="col-lg-10">
                    <div class="main-navigation">
                        <button class="menu-toggle"><span></span><span></span></button>
                        <nav class="header-menu">
                            <ul class="menu food-nav-menu">
                                <li><a href="/">Home</a></li>
                                <li><a href="#contact">Contact</a></li>
                            </ul>
                        </nav>
                        <div class="header-right">
                            <form action="#" class="header-search-form for-des">
                                <input type="search" class="form-input" placeholder="Search Here...">
                                <button type="submit">
                                    <i class="uil uil-search"></i>
                                </button>
                            </form>
                            <a href="javascript:void(0)" class="header-btn header-cart">
                                <i class="uil uil-shopping-bag"></i>
                                <span class="cart-number" id="cart-number-count">0</span>
                            </a>
                            <div class="dropdown">
                                <% if (typeof username !== 'undefined' && username) { %>
                                    <label>
                                        <button onclick="myFunction()" class="header-username" style="color: #ff8243; padding: 4px 8px;">
                                            <b><i><%= username %></i></b>
                                        </button>
                                    </label>
                                    <div id="myDropdown" class="dropdown-content">
                                        <a href="/myorders">My Orders</a>
                                        <a href="/settings">Settings</a>
                                        <a href="/logout">Sign Out</a>
                                    </div>
                                <% } %>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </header>
    <!-- header ends  -->

    <div id="viewport" class="content-padding">
        <div id="js-scroll-content">

            <section class="about-sec section" id="about" style="background-image: url(/images/menu-bg.png);">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="sec-title text-center mb-5">
                                <h2 class="h2-title" style="margin-top: 10px; color: #ff0000;">Payment Failed</h2>
                                <div class="sec-title-shape mb-4">
                                    <img src="/images/title-shape.svg" alt="">
                                </div>
                                <div>
                                    <h3>We're sorry, but your payment could not be processed.</h3>
                                    <h4>If the money has been deducted from your account, you will recive a refund with 3 bussiness days.</h4>
                                </div>
                                <img src="https://media.istockphoto.com/id/1410750503/vector/fail-payment-in-hand-illustration-in-flat-style-declined-money-vector-illustration-on.jpg?s=612x612&w=0&k=20&c=LgL7lHs-W6Nt39M33xzlkSE07Q9-rY6zM5_OXCldzWI=" alt="Payment Failed" class="failed-image">
                                <p>Please check your payment details and try again. Your token ID is <%= typeof tokenId !== 'undefined' ? tokenId : 'Not Available' %> for further reference. You can call us at 7205655157 or email at klglug@kluniversity.in</p>
                                
                                <p class="mt-4">You can return to the cart to retry your payment</p>
                                <button class="sec-btn" style="color: white; background: #ff0000;"><a href="/"
                                        style="color: white;">Return to Cart</a></button><br><br>
                            </div>
                        </div>
                    </div>
                </div>
            </section>

            <!-- footer starts  -->
            <footer class="site-footer" id="contact">
                <div class="top-footer section">
                    <div class="sec-wp">
                        <div class="container">
                            <div class="row">
                                <div class="col-lg-4">
                                    <div class="footer-info">
                                        <div class="footer-logo">
                                            <a href="/">
                                                <img src="/images/file.png" height="120" width="120" alt="KL Eats">
                                            </a>
                                        </div>
                                        <p>Bringing campus dining to your fingertips.</p>
                                        <div class="social-icon">
                                            <ul>
                                                <li><a href="https://www.instagram.com/kl_eats.official?utm_source=qr&igsh=MTZna3N0Y3U1bmRudA==" target="_blank"><i class="uil uil-instagram"></i></a></li>
                                                <li><a href="https://youtube.com/@kleats2024?si=mMXFnxrDZtaed0uz" target="_blank"><i class="uil uil-youtube"></i></a></li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-8">
                                    <div class="footer-flex-box">
                                        <div class="footer-menu food-nav-menu">
                                            <h3 class="h3-title">Links</h3>
                                            <ul class="column-2">
                                                <li><a href="/" class="footer-active-menu">Home</a></li>

                                            </ul>
                                        </div>
                                        <div class="footer-menu">
                                            <h3 class="h3-title">Company</h3>
                                            <ul>
                                                <li><a href="/terms-and-conditions">Terms & Conditions</a></li>
                                                <li><a href="/refund-policy">Refund Policy</a></li>
                                                <li><a href="/contact-us">Contact Us</a></li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="bottom-footer">
                    <div class="container">
                        <div class="row">
                            <div class="col-lg-12 text-center">
                                <div class="copyright-text">
                                    <p>Copyright &copy; 2024 <span class="name" style="color: #f40808;">KL Eats</span> All Rights Reserved.</p>
                                </div>
                            </div>
                        </div>
                        <button class="scrolltop"><i class="uil uil-angle-up"></i></button>
                    </div>
                </div>
            </footer>
        </div>
    </div>

    <!-- jquery  -->
    <script src="/javascripts/jquery-3.5.1.min.js"></script>
    <!-- bootstrap -->
    <script src="/javascripts/bootstrap.min.js"></script>
    <script src="/javascripts/popper.min.js"></script>

    <!-- fontawesome  -->
    <script src="/javascripts/font-awesome.min.js"></script>

    <!-- swiper slider  -->
    <script src="/javascripts/swiper-bundle.min.js"></script>

    <!-- mixitup -- filter  -->
    <script src="/javascripts/jquery.mixitup.min.js"></script>

    <!-- fancy box  -->
    <script src="/javascripts/query.fancybox.min.js"></script>

    <!-- parallax  -->
    <script src="/javascripts/parallax.min.js"></script>

    <!-- gsap  -->
    <script src="/javascripts/gsap.min.js"></script>

    <!-- scroll trigger  -->
    <script src="/javascripts/ScrollTrigger.min.js"></script>
    <!-- scroll to plugin  -->
    <script src="/javascripts/ScrollToPlugin.min.js"></script>
    <!-- rellax  -->
    <!-- <script src="assets/js/rellax.min.js"></script> -->
    <!-- <script src="assets/js/rellax-custom.js"></script> -->
    <!-- smooth scroll  -->
    <script src="/javascripts/smooth-scroll.js"></script>
    <!-- custom js  -->
    <script src="/javascripts/main.js"></script>

</body>

</html>